  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-1": "^6.1.2",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chai": "^3.4.0",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "mocha": "^2.3.3",
