  "description": "Simple modal component for React.",
  "repository": "thejameskyle/react-modal2",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "example": "npm run example:basic & npm run example:animated & npm run example:redux",
    "example:basic": "browserify --debug examples/basic/script.js -o examples/basic/bundle.js -t babelify",
